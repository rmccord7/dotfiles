  "i64": {
    "prefix": "i64",
    "body": ["int64_t"],
    "description": "Code snippet for type"
  },
  "blk": {
    "prefix": "blk",
    "body": ["{", "}"],
    "description": "Code snippet for code block"
  },
  "for": {
    "prefix": "for",
    "body": [
      "for(${Index} = ${1:0}; ${Index} < ${2:length}; ${Index}++)",
      "{",
      "\t$3",
      "}"
    ],
    "description": "Code snippet for 'for' loop"
  },
  "forr": {
    "prefix": "forr",
    "body": [
      "for(int ${i} = ${1:length} - 1; ${i} >= ${2:0}; ${i}--)",
      "{",
      "\t$3",
      "}"
    ],
    "description": "Code snippet for reverse 'for' loop"
  },
  "while": {
    "prefix": "while",
    "body": ["while ($1)", "{", "\t$2", "}"],
    "description": ""
  },
  "if": {
    "prefix": "if",
    "body": ["if($1)", "{", "}"],
    "description": "Code snippet for if statement"
  },
  "else": {
    "prefix": "else",
    "body": ["else", "{", "}"],
    "description": "Code snippet for else statement"
  },
  "else if": {
    "prefix": "else if",
    "body": ["else if($1)", "{", "}"],
    "description": "Code snippet for else-if statement"
  },
  "enum": {
    "prefix": "enum",
    "body": ["enum ${MyEnum}", "{", "\t$1", "};"],
    "description": "Code snippet for enum"
  },
  "#ifdef": {
    "prefix": "#ifdef",
    "body": ["#ifdef ${DEBUG}", "$1", "#endif // ${DEBUG}"],
    "description": "Code snippet for #ifdef"
  },
  "#ifndef": {
    "prefix": "#ifndef",
    "body": ["#ifndef ${1:1}", "$2", "#endif // !$1"],
    "description": "Code snippet for #ifndef"
  },
  "#if": {
    "prefix": "#if",
    "body": ["#ifdef ${1:0}", "$2", "#endif // $1"],
    "description": "Code snippet for #if"
  },
  "struct": {
    "prefix": "struct",
    "body": ["struct ${MyStruct}", "{", "\t$1", "};"],
    "description": "Code snippet for struct"
  },
  "switch": {
    "prefix": "switch",
    "body": ["switch($1)", "{", "\tdefault:", "\t\tbreak;", "}"],
    "description": "Code snippet for switch statement"
  },
  "union": {
    "prefix": "union",
    "body": ["union ${MyUnion}", "{", "\t$1", "};"],
    "description": "Code snippet for union"
  },
  "#inc": {
    "prefix": "#inc",
    "body": ["#include \"$1\""],
    "description": "Code snippet for #include \" \""
  },
  "#inc<": {
    "prefix": "#inc<",
    "body": ["#include <$1>"],
    "description": "Code snippet for #include \" \""
  },
  "#def": {
    "prefix": "def",
    "body": ["#define $1 $2 "],
    "description": "Code snippet for #define \" \""
  }
}
