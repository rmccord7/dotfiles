  "i8": {
    "prefix": "i8",
    "body": ["int8_t"],
    "description": "Code snippet for type"
  },
  "i16": {
    "prefix": "i16",
    "body": ["int16_t"],
    "description": "Code snippet for type"
  },
  "i32": {
    "prefix": "i32",
    "body": ["int32_t"],
    "description": "Code snippet for type"
